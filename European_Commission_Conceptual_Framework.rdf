    <!-- http://data.europa.eu/54i#Actor -->

    <owl:Class rdf:about="&eccf;#Actor">
        <rdfs:label xml:lang="en">Actor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&eccf;#usesAsset"/>
                                <owl:someValuesFrom rdf:resource="&eccf;#Asset"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&eccf;#usesInstrument"/>
                                <owl:someValuesFrom rdf:resource="&eccf;#PolicyInstrument"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&eccf;#usesService"/>
                                <owl:someValuesFrom rdf:resource="&eccf;#Service"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&eccf;#isCapableOf"/>
                        <owl:someValuesFrom rdf:resource="&eccf;#Capability"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&eccf;#isCapableOf"/>
                        <owl:someValuesFrom rdf:resource="&eccf;#Transformation"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&eccf;#isMotivatedBy"/>
                        <owl:someValuesFrom rdf:resource="&eccf;#Motivation"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:isDefinedBy rdf:resource="&eccf;#"/>
    </owl:Class>
    


